{
  "name": "moltin-micro-checkout",
  "version": "1.0.0",
  "description": "⚡️ One-click moltin checkout with Stripe",
  "scripts": {
    "dev": "micro-dev",
    "precommit": "pretty-quick --staged",
    "start": "micro"
  },
  "main": "index.js",
  "author": "Jonathan Steele",
  "license": "MIT",
  "dependencies": {
    "@moltin/sdk": "github:ynnoj/js-sdk",
    "micro": "^9.1.4",
    "micro-cors": "^0.1.0",
    "microrouter": "^3.1.1"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "micro-dev": "^2.2.2",
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "jsxBracketSameLine": true
  },
  "now": {
    "alias": "moltin-micro-checkout.now.sh",
    "dotenv": true,
    "env": {
      "NODE_ENV": "production",
      "MOLTIN_CLIENT_ID": "@moltin-client-id"
    }
  }
}
